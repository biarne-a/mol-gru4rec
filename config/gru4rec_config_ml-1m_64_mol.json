{
  "batch_size": 128,
  "epochs": 24,
  "val_every_n_steps": 500,
  "early_stopping_patience": 3,
  "early_stopping_metric": "val_hr@100",
  "learning_rate": 1e-3,
  "model_config": {
    "embedding_dim": 64,
    "dropout_p_embed": 0.0,
    "dropout_p_gru": 0.0,
    "similarity_config": {
      "type": "mol",
      "query_embedding_dim": 64,
      "item_embedding_dim": 64,
      "dot_product_dimension": 64,
      "query_dot_product_groups": 5,
      "query_dropout_rate": 0.1,
      "query_hidden_dim": 64,
      "query_nonlinearity": "geglu",
      "uid_embed": false,
      "uid_dropout_rate": 0.1,
      "uid_embedding_level_dropout": false,
      "gating_query_fn": true,
      "gating_item_fn": true,
      "gating_qi_dropout_rate": 0.1,
      "gating_item_dropout_rate": 0.1,
      "gating_qi_hidden_dim": 12,
      "gating_query_hidden_dim": 64,
      "gating_item_hidden_dim": 64,
      "gating_combination_type": "glu_silu"
    }
  }
}
