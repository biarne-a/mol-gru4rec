{
  "batch_size": 128,
  "epochs": 24,
  "val_every_n_steps": 500,
  "early_stopping_patience": 3,
  "early_stopping_metric": "val_hr@10",
  "learning_rate": 1e-3,
  "model_config": {
    "embedding_dim": 64,
    "dropout_p_embed": 0.0,
    "dropout_p_gru": 0.0,
    "aux_loss_weights": {"mi_loss": 0.001},
    "similarity_type": "mol",
    "similarity_config": {
      "query_embedding_dim": 64,
      "item_embedding_dim": 64,
      "dot_product_dimension": 64,
      "dot_product_l2_norm": false,
      "query_dot_product_groups": 8,
      "query_dropout_rate": 0.0,
      "query_hidden_dim": 512,
      "query_nonlinearity": "swiglu",
      "uid_embed": false,
      "uid_dropout_rate": 0.0,
      "uid_embedding_level_dropout": false,
      "item_dot_product_groups": 4,
      "item_dropout_rate": 0.1,
      "item_hidden_dim": -1,
      "item_nonlinearity": "swiglu",
      "item_semantic_embed": false,
      "item_semantic_emb_dimension": -1,
      "gating_query_fn": true,
      "gating_item_fn": true,
      "gating_qi_dropout_rate": 0.0,
      "gating_item_dropout_rate": 0.0,
      "gating_qi_hidden_dim": 128,
      "gating_query_hidden_dim": 128,
      "gating_item_hidden_dim": 128,
      "gating_combination_type": "glu_silu"
    }
  }
}
